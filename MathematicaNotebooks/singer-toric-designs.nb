Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"findAllDifferenceSets", "[", 
    RowBox[{"t_", ",", "n_", ",", "m_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"list", "=", 
       RowBox[{"{", "}"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"Length", "@", 
             RowBox[{"DeleteDuplicates", "@", 
              RowBox[{"Table", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"Mod", "[", 
                 RowBox[{
                  RowBox[{
                   SubsuperscriptBox["\[Sum]", 
                    RowBox[{"i", "=", "1"}], "t"], 
                   RowBox[{"p", "[", 
                    RowBox[{"[", 
                    RowBox[{"a", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "]"}], "]"}]}], ",", 
                  "m"}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"a", ",", 
                  RowBox[{"DeleteDuplicates", "[", 
                   RowBox[{"Sort", "/@", 
                    RowBox[{"Tuples", "[", 
                    RowBox[{
                    RowBox[{"Range", "@", "n"}], ",", "t"}], "]"}]}], "]"}]}],
                  "}"}]}], "\[IndentingNewLine]", "]"}]}]}], "\[Equal]", 
            RowBox[{"Binomial", "[", 
             RowBox[{
              RowBox[{"n", "+", "t", "-", "1"}], ",", "t"}], "]"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"AppendTo", "[", 
             RowBox[{"list", ",", "p"}], "]"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"p", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", "0", "}"}], ",", "#"}], "]"}], "&"}], "/@", 
           RowBox[{"DeleteDuplicates", "[", 
            RowBox[{"Sort", "/@", 
             RowBox[{"Tuples", "[", 
              RowBox[{
               RowBox[{"Range", "[", 
                RowBox[{"m", "-", "1"}], "]"}], ",", 
               RowBox[{"n", "-", "1"}]}], "]"}]}], "]"}]}]}], "}"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "@", "list"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
