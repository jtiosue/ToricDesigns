Cell[CellGroupData[{

Cell["Singer set construction (works when n-1 is a prime power)", "Subsection",
 CellChangeTimes->{{3.911914078751017*^9, 3.911914135674327*^9}, {
  3.9119875850392847`*^9, 3.911987586444717*^9}, {3.911987740372383*^9, 
  3.911987740907392*^9}},ExpressionUUID->"c0e0a8d7-8989-4f09-b4c6-\
6490d9616200"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"T", "[", 
    RowBox[{"t_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "\[Theta]", ",", "field", ",", "p", ",", "mod", ",", "prime", ",", 
       "power"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"prime", "=", 
       RowBox[{"First", "@", 
        RowBox[{"First", "@", 
         RowBox[{"FactorInteger", "[", 
          RowBox[{"n", "-", "1"}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "Find", " ", "an", " ", "irreducible", " ", "polynomial", " ", "p", 
         RowBox[{"(", "x", ")"}], " ", "for", " ", "which", " ", 
         RowBox[{"GF", "[", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}], 
           RowBox[{"t", "+", "1"}]], "]"}]}], " ", "=", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"GF", "[", "prime", "]"}], "[", "x", "]"}], " ", "/", " ", 
         RowBox[{"(", 
          RowBox[{"p", 
           RowBox[{"(", "x", ")"}]}], ")"}]}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"p", "=", 
       RowBox[{"FieldIrreducible", "[", 
        RowBox[{
         RowBox[{"GF", "[", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}], 
           RowBox[{"t", "+", "1"}]], "]"}], ",", "x"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"n", "-", "1"}], ")"}], 
         RowBox[{"t", "+", "1"}]], "=", 
        SuperscriptBox["prime", "power"]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"power", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"t", "+", "1"}], ")"}], 
        RowBox[{"Log", "[", 
         RowBox[{"prime", ",", 
          RowBox[{"n", "-", "1"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"To", " ", "do", " ", "arithmetic", " ", "in", " ", 
         RowBox[{"GF", "[", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}], 
           RowBox[{"t", "+", "1"}]], "]"}]}], ",", " ", 
        RowBox[{"we", " ", "do", " ", "arithmetic", " ", "in", " ", 
         RowBox[{
          RowBox[{"GF", "[", "prime", "]"}], "[", "x", "]"}], " ", "and", " ",
          "then", " ", "mod", " ", "by", " ", "p", 
         RowBox[{"(", "x", ")"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"mod", "[", "q_", "]"}], ":=", 
       RowBox[{"PolynomialMod", "[", 
        RowBox[{"q", ",", "p", ",", 
         RowBox[{"Modulus", "\[Rule]", "prime"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "it", " ", "appears", " ", "that", " ", "Mathematica", " ", "picks", 
        " ", "p", " ", "such", " ", "that", " ", "x", " ", 
        "\[IndentingNewLine]", "is", " ", "the", " ", "primitive", " ", 
        "element", " ", 
        RowBox[{"(", 
         RowBox[{
         "see", " ", "the", " ", "first", " ", "section", " ", "of", " ", 
          "this", " ", "notebook"}], ")"}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"\[Theta]", "=", "x"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "this", " ", "is", " ", "all", " ", "the", " ", "nonzero", " ", 
