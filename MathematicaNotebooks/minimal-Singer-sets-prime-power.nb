Cell[CellGroupData[{

Cell["Function to exhaustively verify the constructions", "Section",
 CellChangeTimes->{{3.877559677947171*^9, 3.87755967930188*^9}, {
  3.877559974238315*^9, 
  3.877559983400477*^9}},ExpressionUUID->"6ba8bb12-fce5-4b04-8012-\
1b9fde166fec"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"verify", "[", 
    RowBox[{"set_", ",", "t_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"m", "=", 
        RowBox[{"Length", "@", "set"}]}], ",", "\[IndentingNewLine]", "sums", 
       ",", "\[IndentingNewLine]", 
       RowBox[{"tuples", "=", 
        RowBox[{
         RowBox[{"Sort", "/@", 
          RowBox[{"Tuples", "[", 
           RowBox[{
            RowBox[{"Range", "[", 
             RowBox[{"Length", "@", "set"}], "]"}], ",", "t"}], "]"}]}], "//",
          "DeleteDuplicates"}]}]}], "\[IndentingNewLine]", "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sums", "=", 
       RowBox[{"Table", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{"Total", "@", 
            RowBox[{"set", "[", 
             RowBox[{"[", "tu", "]"}], "]"}]}], ",", 
           FractionBox[
            RowBox[{
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"m", "-", "1"}], ")"}], 
              RowBox[{"t", "+", "1"}]], "-", "1"}], 
            RowBox[{"m", "-", "2"}]]}], "]"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"tu", ",", "tuples"}], "}"}]}], "\[IndentingNewLine]", 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Binomial", "[", 
         RowBox[{
          RowBox[{"m", "+", "t", "-", "1"}], ",", "t"}], "]"}], "\[Equal]", 
        RowBox[{"Length", "@", 
         RowBox[{"DeleteDuplicates", "@", "sums"}]}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Length", "@", "tuples"}], "\[Equal]", 
       RowBox[{"Length", "@", 
        RowBox[{"DeleteDuplicates", "@", "sums"}]}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8775594333131933`*^9, 3.8775596191999683`*^9}, {
  3.8775599867217503`*^9, 3.877559987181201*^9}, {3.8775602145653763`*^9, 
